  	<script>
      // Preloader
      let progress = 0;
      let progressNumber = document.querySelector(
        ".preloader .progress-number"
      );

      function updateProgress(target, duration) {
        let interval = 20;
        let step = (target - progress) / (duration / interval);

        let progressInterval = setInterval(() => {
          progress += step;
          if (progress >= target) {
            progress = target;
            clearInterval(progressInterval);
          }
          progressNumber.textContent = Math.round(progress);
        }, interval);
      }

      updateProgress(80, 250);

      window.addEventListener("load", () => {
        updateProgress(100, 250);
      });
    </script>

